
public class PrimitiveConverter {
	
	
	
	public static Object getFromString(String primitive) {
		
		try {
			return Byte.valueOf(primitive);
		}
		catch(NumberFormatException e) {}
		
		try {
			return Integer.valueOf(primitive);
		}
		catch(NumberFormatException e) {}
		
		
		
		
		
		
		
		
		
	}

}
